                                    h3 Description
                    div.panel-collapse.collapse(id="theone")
                        .panel-body
                            h2 Contexte du projet
                            p Blablabla
                            br
                            h2 Skills
                            p Les skills
                            br

                .panel.panel-default
                    .panel-heading(style='background:#fff')
                        .row(data-toggle="collapse", data-parent="#accor", href="#thetwo",onclick="return false;")
                            .col-xs-6.col-xs-offset-1(style='border-right:1px dashed #778899')
                                h2 Titre du stage
                                h3 Description
                            .col-xs-4(style="text-align:left;")
                                h2 AERODRONES
                                h5 Four month, 2013
                    div.panel-collapse.collapse(id="thetwo")
                        .panel-body(style='background:#778899;color:white;')
                            h2 Contexte du projet
                            p Blablabla
                            br
                            h2 Skills
                            p Les skills
                            br
                .panel.panel-default
                    .panel-heading(style='background:#778899;color:white;')
                        .row(data-toggle="collapse", data-parent="#accor", href="#thethredd",onclick="return false;")
                            .col-xs-4(style="text-align:right")
                                h2 SOCATA
                                h5 One month, 2009
                            .col-xs-6(style="margin-left:30px;border-left:1px dashed white;")
                                h2 Titre du stage
                                h3 Description
                            a CrashTEst 2
                    div.panel-collapse.collapse(id="thethredd")
                        .panel-body
                            h2 Contexte du projet
                            p Blablabla
                            br
                            h2 Skills
                            p Les skills
                            br



        .emptyBox(style="height:500px;background:inherit")

    div(style="background: url(land.jpg) no-repeat center center fixed; -webkit-background-size: cover;background-size:cover;-moz-background-size:cover")
        section.box(id="skills")
            div(style="text-align:center")
                span(class="fa-stack fa-5x")
                    i(class="fa fa-square fa-stack-2x")
                    i(class="fa fa-keyboard-o fa-stack-1x fa-inverse ")
                h1 COMPUTER SKILLS
            h2(style="margin-bottom:20px") Computer Skills
            h4 Langages
            hr(class="style-one")
            .row
                button.btn.btn-default.col-md-2(ng-repeat="skill in skillLangages",style="margin-left:10px; margin-bottom:10px") {{skill.nom}}
                    br
                    .progress.progress-striped.active(style="margin-top:20px")
                        .progress-bar(style="width:{{skill.niveau}}%")

            h4 Front-end
            hr(class="style-one")
            .row
                button.btn.btn-default.col-md-2(ng-repeat="skill in skillFront",style="margin-left:10px; margin-bottom:10px") {{skill.nom}}
                    br
                    .progress.progress-striped.active(style="margin-top:20px")
                        .progress-bar(style="width:{{skill.niveau}}%")

            h4 Back-end
            hr(class="style-one")
            .row
                button.btn.btn-default.col-md-2(ng-repeat="skill in skillFront",style="margin-left:10px; margin-bottom:10px") {{skill.nom}}
                    br
                    .progress.progress-striped.active(style="margin-top:20px")
                        .progress-bar(style="width:{{skill.niveau}}%")

            h4 Other
            hr(class="style-one")
            .row
                button.btn.btn-default.col-md-2(ng-repeat="skill in skillOther",style="margin-left:10px; margin-bottom:10px") {{skill.nom}}
                    br
                    .progress.progress-striped.active(style="margin-top:20px")
                        .progress-bar(style="width:{{skill.niveau}}%")

        .emptyBox(style="height:500px;background:inherit")

