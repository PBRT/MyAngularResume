div(ng-init="init()", anim="fadeIn")

    div(style="background: url(land.jpg) no-repeat center center fixed;-webkit-background-size: cover;background-size:cover;-moz-background-size:cover")
        section.box(id="home")
            div(style="text-align:center;width:100%")

                    span(class="fa-stack fa-5x")
                        i(class="fa fa-square fa-stack-2x")
                        i(class="fa fa-bolt fa-stack-1x fa-inverse ")
                    h1 {{home}}
                    h4(style="font-style:italic") {{homeDescription}}

            .row(style="text-align:center;margin-top:80px")
                .col-xs-4
                    span(class="fa-stack fa-4x")
                        i(class="fa fa-square fa-stack-2x")
                        i(class="fa fa-cloud fa-stack-1x fa-inverse ")
                    h3 Back End
                    h5(style='font-style:italic') RESTful APIS development based on Node.js
                .col-xs-4
                    span(class="fa-stack fa-4x")
                        i(class="fa fa-square fa-stack-2x")
                        i(class="fa fa-mobile fa-stack-1x fa-inverse ")
                    h3 Mobile Development
                    h5(style="font-style:italic") Native and Web Android Application
                .col-xs-4
                    span(class="fa-stack fa-4x")
                        i(class="fa fa-square fa-stack-2x")
                        i(class="fa fa-desktop fa-stack-1x fa-inverse ")
                    h3 User Interface Design
                    h5(style="font-style:italic") User Experience First


                //div(id='canvas', ng-click='draw()')
                        span(class="fa-stack fa-4x")
                            i(class="fa fa-square fa-stack-2x")
                            i(class="fa fa-desktop fa-stack-1x fa-inverse ")
                        h3 User Interface Design
                        h2 TEst

        .emptyBox(style="height:500px;background:inherit")

    div(style="background: url(land.jpg) no-repeat center center fixed;-webkit-background-size: cover;background-size:cover;-moz-background-size:cover")
        section.box(id="education")
            div(style="text-align:center;margin-bottom:50px")
                span(class="fa-stack fa-5x")
                    i(class="fa fa-square fa-stack-2x")
                    i(class="fa fa-graduation-cap fa-stack-1x fa-inverse ")
                h1 EDUCATION
            .row
                .col-xs-4
                    .panel.panel-default.specific(style="border-radius: 10px;", ng-click="changeMode(0)")
                        .panel-body
                            h3(style="text-align:center") Education 1
                            hr(class="style-one")
                            p Details ...
                .col-xs-4
                    .panel.panel-default.specific(style="border-radius: 10px;",ng-click="changeMode(1)")
                        .panel-body
                            h3(style="text-align:center") Education 2
                            hr(class="style-one")
                            p Details ...
                .col-xs-4
                    .panel.panel-default.specific(style="border-radius: 10px;",ng-click="changeMode(2)")
                        .panel-body
                            h3(style="text-align:center") Education 3
                            hr(class="style-one")
                            p Details ...
            .row
                .col-xs-12
                    .panel.panel-default(style="border-radius: 10px;")
                        .panel-body
                            p Details ...
                            //details one
                            div(id="detailsOne", ng-show="mode==0")
                                p Details One

                            //details one
                            div(id="detailsTwo", ng-show="mode==1")
                                p Details Two

                            //details one
                            div(id="detailsThree", ng-show="mode==2")
                                p Details Three

        .emptyBox(style="height:500px;background:inherit")


    div(style="background: url(land.jpg) no-repeat center center fixed;-webkit-background-size: cover;background-size:cover;-moz-background-size:cover")
        section.box(id="experience")
            div(style="text-align:center")
                span(class="fa-stack fa-5x")
                    i(class="fa fa-square fa-stack-2x")
                    i(class="fa fa-road fa-stack-1x fa-inverse ")
                h1 EXPERIENCE

            .panel-group(id="accor")
                .panel.panel-default
                    .panel-heading(style='background:#778899;color:white;')
