div(id="about",class="modal fade",style="display: none;margin-top:200px", aria-hidden="true")
    div(class="modal-dialog modal-content")
        div(class="modal-header")
            button(type="button", data-dismiss="modal" ,aria-hidden="true", class="close") Close
            h2(class="modal-titleAbout") Blog Page
        div(class="modal-body")
            p In construction


.container-fluid(style="overflow:hidden!important;margin-top:-50px", ng-init="init()")
    div(end-transition="fadeOut")
        .row.grow
                h1.col-xs-3.col-xs-offset-3.swag(anim="slideInLeft") Pierre
                h1.col-xs-3.swag(anim="slideInRight") BEARD
            .col-xs-3
        .col-xs-10.col-xs-offset-1(anim-end="fadeIn")
            h5(style="text-align:center;font-style:italic;font-size:30px") Mobility Developer
        .row(anim-end="fadeIn")
            .col-xs-5
                hr(class="style-one")
            .col-xs-2
                h4(style="text-align:center") xxxx X xxxx
            .col-xs-5
                hr(class="style-one")

    .row(style="margin-top:40px; margin-bottom:30px")
        .col-xs-5.rect(data-toggle="modal", data-target="#about",anim-end="slideInLeft",end-transition="slideOutLeft")
            h2.pull-right(anim-end="slideInDown",style="font-size:50px;display:inline;text-align:center;margin-top:70px;text-shadow: -1px 0 rgba(37,49,75,0.75), 0 1px rgba(37,49,75,0.75), 1px 0 rgba(37,49,75,0.75), 0 -1px rgba(37,49,75,0.75);") BLOG
        .col-xs-5.col-xs-offset-3.rect(ng-click="aboutMe()", anim-end="slideInRight",end-transition="slideOutRight",style="margin-top:50px")
            h2.pull-left(end-transition="slideOutDown",anim-end="slideInUp",style="font-size:50px;display:inline;text-align:center;margin-top:70px;text-shadow: -1px 0 rgba(37,49,75,0.75), 0 1px rgba(37,49,75,0.75), 1px 0 rgba(37,49,75,0.75), 0 -1px rgba(37,49,75,0.75);") ABOUT ME